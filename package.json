  "scripts": {
    "fmt": "prettier --bracketSpacing false --jsxBracketSameLine false --single-quote --write 'app/src/**/*.js'",
    "clean": "rimraf dist",
    "clean:desktop": "rimraf dist/desktop",
    "clean:cordova": "rimraf dist/cordova",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:desktop": "npm run clean:desktop && NODE_ENV=production node desktop/build.js",
    "build:desktop:assets": "npm run clean:desktop && NODE_ENV=production webpack --config webpack.config.desktop.prod.js",
    "build:cordova": "npm run clean:cordova && NODE_ENV=production webpack --config webpack.config.cordova.prod.js",
    "start": "node server/dev.js",
    "test": "echo 'TODO'",
    "lint": "eslint src",
    "flow": "flow; [[ $?=0||$?=2 ]]",
    "precommit": "lint-staged",
    "prettier": "prettier --write --single-quote 'app/**/*.js'"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
